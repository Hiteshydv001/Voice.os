# Backend Environment Variables
# Rename this file to .env and fill in your actual values

# OpenAI API Key (Required for voice agent)
# Get it from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Public URL where backend is hosted (for webhooks)
PUBLIC_URL=http://localhost:5050

# Twilio Configuration (Required for phone calls)
# Get from: https://console.twilio.com/
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token

# Port for backend server (optional)
PORT=5050